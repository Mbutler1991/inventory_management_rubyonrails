<h1>Categories</h1>
<%#list of categories names%>
<ul>
  <% @categories.each do |category| %>
    <li>
        <%= category.name %>
        <%= link_to "Edit", edit_category_path(category), class: 'button-small' %>
    </li>
  <% end %>
</ul>

<%= link_to "New Category", new_category_path, class: 'button' %>
