<h1>Suppliers</h1>
<%#list of categories names%>
<ul>
  <% @suppliers.each do |supplier| %>
    <li>
        <%= link_to supplier.name, supplier_path(supplier) %>
        <%= link_to "Edit", edit_supplier_path(supplier), class: 'button-small' %>
    </li>
  <% end %>
</ul>

<%= link_to "New Supplier", new_supplier_path, class: 'button' %>